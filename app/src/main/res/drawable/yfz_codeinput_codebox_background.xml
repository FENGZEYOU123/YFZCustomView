<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval"
            android:useLevel="true">
            <size
                android:width="40dp"
                android:height="40dp" />

            <solid android:color="@color/colorTransparent" />
            <stroke
                android:width="1dp"
                android:color="@color/colorBlack" />
        </shape>
    </item>

</layer-list>